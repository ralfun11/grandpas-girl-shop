#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/regjsparser@0.6.6/node_modules/regjsparser/bin/parser/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/regjsparser@0.6.6/node_modules/regjsparser/bin/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/regjsparser@0.6.6/node_modules/regjsparser/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/regjsparser@0.6.6/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules:/Users/ralfun11/Projects/node_modules:/Users/ralfun11/node_modules:/Users/node_modules:/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/node_modules/regjsparser/bin/parser/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/node_modules/regjsparser/bin/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/node_modules/regjsparser/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../regjsparser/bin/parser" "$@"
else 
  exec node  "$basedir/../regjsparser/bin/parser" "$@"
fi
