#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/jsesc/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/jsesc@2.5.2/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules:/Users/ralfun11/Projects/node_modules:/Users/ralfun11/node_modules:/Users/node_modules:/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/node_modules/jsesc/bin/jsesc/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/node_modules/jsesc/bin/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/node_modules/jsesc/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jsesc/bin/jsesc" "$@"
else 
  exec node  "$basedir/../jsesc/bin/jsesc" "$@"
fi
