#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/next@10.0.1_react-dom@17.0.1+react@17.0.1/node_modules/next/dist/bin/next/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/next@10.0.1_react-dom@17.0.1+react@17.0.1/node_modules/next/dist/bin/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/next@10.0.1_react-dom@17.0.1+react@17.0.1/node_modules/next/dist/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/next@10.0.1_react-dom@17.0.1+react@17.0.1/node_modules/next/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/next@10.0.1_react-dom@17.0.1+react@17.0.1/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/.pnpm/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules:/Users/ralfun11/Projects/node_modules:/Users/ralfun11/node_modules:/Users/node_modules:/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/next/dist/bin/next/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/next/dist/bin/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/next/dist/node_modules:/Users/ralfun11/Projects/grandpas-girl-shop/node_modules/next/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else 
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
